diff -urN dovecot-1.2.0/dovecot-libsieve/src/lib-sieve/plugins/enotify/ext-enotify-common.c dovecot-1.2.0-sieve/dovecot-libsieve/src/lib-sieve/plugins/enotify/ext-enotify-common.c
