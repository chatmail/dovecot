+/*
+ * Capability registries
+ */
+
+struct sieve_extension_capabilities {
+	const char *name;
+
+	const struct sieve_extension *extension;
+
+	const char *(*get_string)(void);	
+};
+
+void sieve_extension_capabilities_register
