diff -urN dovecot-1.2.0/dovecot-libsieve/src/lib-sieve/plugins/variables/ext-variables-limits.h dovecot-1.2.0-sieve/dovecot-libsieve/src/lib-sieve/plugins/variables/ext-variables-limits.h
