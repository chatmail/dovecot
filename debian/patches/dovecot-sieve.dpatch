+bool sieve_operand_optional_present
+(struct sieve_binary *sbin, sieve_size_t *address)
+{	
+	sieve_size_t tmp_addr = *address;
+	unsigned int op = -1;
+	
+	if ( sieve_binary_read_byte(sbin, &tmp_addr, &op) && 
+		(op == SIEVE_OPERAND_OPTIONAL) ) {
+		*address = tmp_addr;
+		return TRUE;
+	}
+	
+	return FALSE;
+}
