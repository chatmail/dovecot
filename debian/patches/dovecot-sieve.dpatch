+/* Copyright (c) 2002-2009 Dovecot Sieve authors, see the included COPYING file 
+ */
+
+#include "lib.h"
+#include "array.h"
+#include "home-expand.h"
+#include "deliver.h"
+#include "duplicate.h"
+#include "smtp-client.h"
+
+#include "sieve.h"
+
+#include "lda-sieve-plugin.h"
+
+#include <stdlib.h>
+#include <sys/stat.h>
+#include <dirent.h>
