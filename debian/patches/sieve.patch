+(const struct sieve_dumptime_env *denv, sieve_size_t *address)
+{
+	const struct sieve_operation *op = &denv->oprtn;
+
+	sieve_code_dumpf(denv, "%s:", sieve_operation_mnemonic(op));
+
+	sieve_code_descend(denv);
